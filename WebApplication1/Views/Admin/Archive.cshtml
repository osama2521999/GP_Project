
@{
    ViewBag.Title = "Archive";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<head>
    <!-- Vendor CSS-->
    <link href="~/Content/AddDoctor_css/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Content/AddDoctor_css/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Content/AddDoctor_css/css/main.css" rel="stylesheet" media="all">
</head>

<body>


    


    <div class="page-wrapper bg-white p-t-130 p-b-100 font-poppins">
        <div class="wrapper wrapper--w680">
            <div class="card card-4">
                <div class="card-body">

                    <form method="POST" >
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Name</label>
                                    <input class="input--style-4" type="text" name="Name">
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Max Team</label>
                                    <input class="input--style-4" type="text" name="Max Team">
                                </div>
                            </div>
                        </div>
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Email</label>
                                    <div class="input-group-icon">
                                        <input class="input--style-4 " type="email" name="Email">
                                    </div>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Required Language</label>
                                    <div class=" rs-select2 js-select-simple select--no-search">
                                        <select name="subject">
                                            <option disabled="disabled" selected="selected">Choose option</option>
                                            <option>Subject 1</option>
                                            <option>Subject 2</option>
                                            <option>Subject 3</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Password</label>
                                    <div class="input-group-icon">
                                        <input class="input--style-4 " type="password" name="Password">
                                    </div>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Required Technology</label>
                                    <div class="rs-select2 js-select-simple select--no-search">
                                        <select name="subject">
                                            <option disabled="disabled" selected="selected">Choose option</option>
                                            <option>Subject 1</option>
                                            <option>Subject 2</option>
                                            <option>Subject 3</option>
                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Section</label>
                                    <input class="input--style-4" type="text" name="Section">
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="input-group">
                                    <label class="label">Id</label>
                                    <input class="input--style-4" type="text" name="Id">
                                </div>
                            </div>
                        </div>

                        <div class="p-t-15">
                            <button class="btn btn--radius-2 btn--blue" type="submit" >
                                <span class="glyphicon glyphicon-ok"></span>
                                Submit
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>


    <!-- Jquery JS-->
    <script src="~/Content/AddDoctor_css/vendor/jquery/jquery.min.js"></script>

    <!-- Vendor JS-->
    <script src="~/Content/AddDoctor_css/vendor/select2/select2.min.js"></script>
    <script src="~/Content/AddDoctor_css/vendor/datepicker/moment.min.js"></script>
    <script src="~/Content/AddDoctor_css/vendor/datepicker/daterangepicker.js"></script>

    <!-- Main JS-->
    <script src="~/Content/AddDoctor_css/js/global.js"></script>

</body>

